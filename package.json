    "parcel": "^2.0.0-beta.1",
    "postcss": "^8.1.6",
    "postcss-color-function": "^4.1.0",
    "postcss-modules": "^3.2.2",
    "postcss-nested": "^5.0.1",
    "react-router-dom": "^5.2.0",
    "rimraf": "^3.0.2"
  }
}
